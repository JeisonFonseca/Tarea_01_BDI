@model List<Tarea_1.Models.Empleado>


<hi>Empleados</hi>

<p>
	<a asp-action="Create">Crear </a>
</p>

<table class="table">
	<thead>
		<tr>
			<th>Cedula</th>
			<th>Nombre</th>
			<th>Apellido 1</th>
			<th>Apellido 2</th>
			<th>Rol</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var empleado in Model)
		{
			<tr>
				<td>@Html.DisplayFor(m=>empleado.Cedula)</td>
				<td>@Html.DisplayFor(m=>empleado.Nombre)</td>
				<td>@Html.DisplayFor(m=>empleado.Apellido1)</td>
				<td>@Html.DisplayFor(m=>empleado.Apelllido2)</td>
				<td>@Html.DisplayFor(m=>empleado.Rol)</td>
			</tr>
		}
	</tbody>
</table>